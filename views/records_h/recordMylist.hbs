                          <td class="text-center">{{ this.startTime }}</td>
                          <td class="text-center">{{ this.endTime }}</td>
                          <td class="text-center">{{ this.stackedDistance }}</td>
                          <td class="text-center">{{ this.startx }}</td>
                          <td class="text-center">{{ this.starty }}</td>
                          <td class="text-center">{{ this.endx }}</td>
                          <td class="text-center">{{ this.endy }}</td>
                          <td class="text-center">{{ this.createdAt }}</td>
                        </tr>
                        {{/each}}
